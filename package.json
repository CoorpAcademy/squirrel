{
  "name": "@coorpacademy/squirrel",
  "version": "1.4.4",
  "description": "High level client for Coorpacademy's squirrel service",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run nyc",
    "lint": "eslint .",
    "ava": "RUNNING_UNIT_TESTS=true ava",
    "nyc": "RUNNING_UNIT_TESTS=true nyc ava --all",
    "nyc:lcov": "npm run nyc -- --reporter=lcov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coorpacademy/squirrel.git"
  },
  "keywords": [
    "squirrel"
  ],
  "author": "dev@coorpacademy.com",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "bugs": {
    "url": "https://github.com/coorpacademy/squirrel/issues"
  },
  "homepage": "https://github.com/coorpacademy/squirrel#readme",
  "dependencies": {
    "bluebird": "^3.3.4",
    "bluebird-retry": "0.5.3",
    "debug": "^2.2.0",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "node-etcd": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.4.0",
    "eslint-config-coorpacademy": "^1.0.0",
    "eslint-plugin-ava": "^1.2.0",
    "http-proxy": "^1.13.2",
    "nyc": "^6.1.1"
  },
  "bin": {
    "squirrel": "src/bin/cli.js"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ]
  },
  "nyc": {
    "exclude": [
      "src/**/test/**/*.js",
      "coverage"
    ]
  }
}
