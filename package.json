{
  "name": "@coorpacademy/squirrel",
  "version": "1.4.5",
  "description": "High level client for Coorpacademy's squirrel service",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf .nyc_output coverage lib",
    "prepublish": "npm run build",
    "build": "babel src --out-dir lib -s",
    "test": "npm run lint && npm run nyc",
    "lint": "eslint .",
    "ava": "RUNNING_UNIT_TESTS=true ava",
    "nyc": "RUNNING_UNIT_TESTS=true nyc ava --all",
    "nyc:lcov": "npm run nyc -- --reporter=lcov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coorpacademy/squirrel.git"
  },
  "keywords": [
    "squirrel"
  ],
  "author": "dev@coorpacademy.com",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "bugs": {
    "url": "https://github.com/coorpacademy/squirrel/issues"
  },
  "homepage": "https://github.com/coorpacademy/squirrel#readme",
  "dependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "debug": "^2.2.0",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "node-etcd": "^5.0.0",
    "rxjs": "^5.0.0-beta.6"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-eslint": "^6.0.3",
    "coveralls": "^2.11.8",
    "eslint": "^2.4.0",
    "eslint-config-coorpacademy": "^2.0.2",
    "eslint-plugin-ava": "^2.2.1",
    "nyc": "^6.1.1"
  },
  "bin": {
    "squirrel": "lib/bin/cli.js"
  },
  "ava": {
    "files": [
      "src/**/test/**/*.js",
      "!node_module/**/*"
    ],
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit",
    "timeout": "10m"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "**/test/**/*"
    ],
    "require": [
      "babel-core/register"
    ],
    "all": true
  }
}
